<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.connection.driver_class">org.h2.Driver</property>
        <property name="hibernate.connection.password">1234</property>
        <property name="hibernate.connection.url">jdbc:h2:tcp://localhost/~/app</property>
        <property name="hibernate.connection.username">ayu</property>
        <property name="hibernate.dialect">org.hibernate.dialect.H2Dialect</property>
        <property name="hibernate.show_sql">true</property>
        
        <mapping resource="org/bean/Customer.hbm.xml"/>
         <mapping resource="org/bean/Account.hbm.xml"/>
          <mapping resource="org/bean/FundTransfer.hbm.xml"/>
           <mapping resource="org/bean/Transaction.hbm.xml"/>
    </session-factory>
</hibernate-configuration>
